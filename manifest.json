{
  "name": "page-cleaner",
  "version": "0.9.0",
  "icons": {
    "128": "img/avd.png"
  },
  "manifest_version": 3,
  "description": "chrome浏览器页面清爽工具",
  "content_scripts": [
    {
      "matches": [
        "https://www.jb51.net/*"
      ],
      "js": [
        "js/jquery.2.1.4.js",
        "js/ad_remover/jb51.js"
      ],
      "runat": "document_end"
    },
    {
      "matches": [
        "https://blog.csdn.net/*"
      ],
      "js": [
        "js/jquery.2.1.4.js",
        "js/ad_remover/csdn.js",
        "js/onion/csdn_clear.js"
      ],
      "runat": "document_end"
    },
    {
      "matches": [
        "https://www.baidu.com/*"
      ],
      "js": [
        "js/jquery.2.1.4.js",
        "js/onion/baidu_index.js",
        "js/ad_remover/baidu_search.js"
      ],
      "runat": "document_end"
    },
    {
      "matches": [
        "https://www.bejson.com/*"
      ],
      "js": [
        "js/jquery.2.1.4.js",
        "js/onion/bejson.js"
      ],
      "runat": "document_end"
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/jquery.2.1.4.js",
        "js/onion/global.js"
      ],
      "runat": "document_end"
    }
  ],
  "host_permissions": [
    "https://www.jb51.net/*",
    "https://blog.csdn.net/*",
    "https://www.bejson.com/*",
    "https://www.baidu.com/",
    "https://*/*",
    "http://*/*",
    "http://localhost:*/*"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "img/avd.png"
    }
  }
}